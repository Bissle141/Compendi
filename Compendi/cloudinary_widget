function showUploadWidget() { cloudinary.openUploadWidget({    cloudName: "<cloud name>",    uploadPreset: "<upload preset>",    sources: [        "url",        "camera",        "image_search",        "dropbox",        "instagram",        "shutterstock",        "getty",        "local",        "google_drive",        "facebook",        "istock",        "unsplash"    ],    googleApiKey: "<image_search_google_api_key>",    showAdvancedOptions: true,    cropping: true,    multiple: false,    defaultSource: "url",    styles: {        palette: {            window: "#F5F2EA",            windowBorder: "#F5F2EA",            tabIcon: "#FDC886",            menuIcons: "#CBCD88",            textDark: "#010101",            textLight: "#FFFFFF",            link: "#9A978F",            action: "#FDC886",            inactiveTabIcon: "#CBCD88",            error: "#E27F78",            inProgress: "#82A8D4",            complete: "#8BC192",            sourceBg: "#FFFFFF"        },        fonts: {            default: null,            "sans-serif": {                url: null,                active: true            }        }    }}, (err, info) => {   if (!err) {         console.log("Upload Widget event - ", info);   }  }); }